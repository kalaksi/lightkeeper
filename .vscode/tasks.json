{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "cargo build",
			"command": "cargo build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": ["$rustc"]
		},
		{
			"type": "shell",
			"label": "cargo test",
			"command": "cargo test",
			"group": {
				"kind": "build",
				"isDefault": false
			},
			"problemMatcher": ["$rustc"]
		}
	]
}
